{
  "name": "datepicker-custom-react",
  "private": false,
  "version": "2.1.5",
  "type": "module",
  "description": "Customizable datepicker for react",
  "license": "MIT",
  "author": "Zang983",
  "keywords": [
    "Datepicker",
    "Customizable"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/datepicker-custom-react.es.ts",
      "require": "./dist/datepicker-custom-react.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/datepicker-custom-react.ts",
  "module": "./dist/datepicker-custom-react.es.ts",
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@types/node": "^20.4.8",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.25.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-linter": "^2.0.2",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "peerDependencies": {
    "react": "18.2.0 || >=18.x",
    "react-dom": "18.2.0 || >=18.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Zang983/datepicker-custom-react-module.git"
  }
}
